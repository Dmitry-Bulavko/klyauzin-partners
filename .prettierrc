{
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": false,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "htmlWhitespaceSensitivity": "css",
  "endOfLine": "lf",
  "overrides": [
    {
      "files": "*.njk",
      "options": {
        "parser": "html",
        "htmlWhitespaceSensitivity": "strict",
        "bracketSameLine": false,
        "singleAttributePerLine": true
      }
    }
  ]
}
